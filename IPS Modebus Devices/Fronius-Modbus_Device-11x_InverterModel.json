    "VirtualAddresses": [
        {
            "Active": true,
            "Ident": "VA_Efficiency",
            "Name": "Efficiency",
            "Profile": "Fronius.Percent.1",
            "ReadAction": "$varId_ACP = IPS_GetObjectIDByIdent(\"40092_AC_Power\", $_IPS['TARGET']);\r\n$varId_DCP = IPS_GetObjectIDByIdent(\"40108_DC_Power\", $_IPS['TARGET']);\r\n$ac_power = GetValueFloat($varId_ACP);\r\n$dc_power = GetValueFloat($varId_DCP);\r\nif ($dc_power > 0) {\r\n    $efficiency = ($ac_power / $dc_power) * 100;\r\n    return round($efficiency, 2);\r\n} else {\r\n    return 0.0;\r\n}",
            "Translation": [],
            "VariableType": 2,
            "WriteAction": ""
        },
        {
            "Active": true,
            "Ident": "VA_UpdateCnt",
            "Name": "Update Cnt",
            "Profile": "",
            "ReadAction": "$varId = @IPS_GetObjectIDByIdent(\"VA_UpdateCnt\", $_IPS['TARGET']);\r\nif ($varId === false) { return -1; } else { return GetValueInteger($varId)+1; }",
            "Translation": [],
            "VariableType": 1,
            "WriteAction": ""
        },
        {
